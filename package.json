{
  "name": "madsciencebulgaria.github.io",
  "version": "1.0.0",
  "description": "This is the official website of Mad Science Ltd., Bulgaria. It is a SPA that displays the offered services from the company, the people that work here and the pricing for services. ",
  "dependencies": {
    "@material-ui/codemod": "^1.1.0",
    "@material-ui/core": "^1.5.1",
    "@material/top-app-bar": "^0.40.0",
    "@material/typography": "^0.39.0",
    "firebase": "^4.13.1",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.20.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^16.7.0",
    "react-localization": "^1.0.13",
    "react-modal": "^2.3.2",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "react-test-renderer": "^16.7.0",
    "react-transition-group": "^1.2.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "source-map-explorer": "^1.6.0",
    "uuid": "^3.3.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-js": "webpack-dev-server --progress",
    "start": "npm-run-all start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.12.3",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "jest-enzyme": "^4.2.0",
    "lint-staged": "^7.3.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-app-env": "^1.2.3",
    "redux-devtools": "^3.5.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "extract-loader": "^1.0.2",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.3"
  }
}